{
  "name": "pith",
  "description": "pith commands ",
  "version": "1.0.0",
  "author": "ashutoshpith @ashutoshpith",
  "bin": {
    "pith": "./bin/run"
  },
  "bugs": "https://github.com/ashutoshpith/pith/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "@types/fs-extra": "^8.1.0",
    "fastify": "^2.13.0",
    "open": "^7.0.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.18",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ashutoshpith/pith",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pith",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "apps:favorites": {
        "description": "manage favorite apps"
      },
      "config": {
        "description": "manage heroku config variables"
      }
    }
  },
  "repository": "ashutoshpith/pith",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start": "./bin/run"
  },
  "types": "lib/index.d.ts"
}
